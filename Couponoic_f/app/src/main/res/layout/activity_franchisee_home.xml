<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.ramesh.couponoic_f.FranchiseeHomeActivity"
    android:background="@drawable/bg45">

    <ImageButton
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:text="Visible"
        android:id="@+id/btnVisible"
        android:background="@drawable/greenf1"
        android:layout_alignTop="@+id/btnDisable"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:text="Disable"
        android:id="@+id/btnDisable"
        android:background="@drawable/redf2"
        android:layout_marginTop="98dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/btnVisible"
        android:layout_alignStart="@+id/btnVisible" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="hello how are u fine ne khana khake jana hA"
        android:id="@+id/txtVisibilityMsg"
        android:textColor="#fff5fff5"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp" />


    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="Logout"
        android:id="@+id/btnLogout"
        android:textSize="15dp"
        android:background="#ff1e90ff"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="8dp" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="400dp"
        android:layout_above="@+id/btnLogout"
        android:id="@+id/linearLayout">

        <Button
            android:layout_width="170dp"
            android:layout_height="41dp"
            android:text="Received Coupons"
            android:id="@+id/btnReceivedCoupons"
            android:textSize="15dp"
            android:background="#ffff8306"

            android:layout_alignTop="@+id/btnSearchCoupon" />

        <Button
            android:layout_width="fill_parent"
            android:layout_height="40dp"
            android:text="Inquire Coupon"
            android:id="@+id/btnSearchCoupon"
            android:background="#ff38ff06"

            android:layout_below="@+id/btnVisible"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="8dp" />
    </LinearLayout>

</RelativeLayout>
